
<div class="job">
    <%= console.log('---------------------------------') %>
    <%= console.log(job) %>
    <div class="eyebrow_wrap">
        <h4 class="date"> <%= job?.start + (job?.end ? ' - ' + job.end : '') %> </h4>
        <h4 class="company">  <%= (job?.company ? "Company: "+ job.company : "Personal project") %> </h4>
    </div>
    <h3>  <%= job?.title %> </h3>
    <ul class="secondary_listzzz">
        <% job?.descriptions.forEach( info => { %>
            <li> <%- info %> </li>
        <% }) %>
    </ul> 
</div>